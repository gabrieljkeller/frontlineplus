{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "tesseract"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-process-env": "off",
    "id-length": "off"
  }
}